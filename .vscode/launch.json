{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "üîß Debug: Default (Listener)",
      "type": "debugpy",
      "request": "attach",
      "connect": {
        "host": "localhost",
        "port": 5678
      },
      "justMyCode": false,
      "pathMappings": [
        {
          "localRoot": "${workspaceFolder}",
          "remoteRoot": "."
        }
      ],
      "preLaunchTask": "debug-send: default"
    },
    {
      "name": "üèóÔ∏è Debug: Custom (Listener)",
      "type": "debugpy",
      "request": "attach",
      "connect": {
        "host": "localhost",
        "port": 5678
      },
      "justMyCode": false,
      "subProcess": false,
      "presentation": {
        "hidden": false,
        "group": "",
        "order": 1
      },
      "redirectOutput": false,
      "showReturnValue": false,
      "pathMappings": [
        {
          "localRoot": "${workspaceFolder:Project}",
          "remoteRoot": "."
        }
      ],
      "preLaunchTask": "debug-send: custom"
    },
    {
      "name": "üîå Attach to Python Process (Manual)",
      "type": "debugpy",
      "request": "attach",
      "connect": {
        "host": "localhost",
        "port": 5678
      },
      "justMyCode": false,
      "pathMappings": [
        {
          "localRoot": "${workspaceFolder}",
          "remoteRoot": "."
        }
      ]
    }
  ]
}